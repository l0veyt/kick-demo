<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>kick-demo</display-name>
  <servlet>
    <servlet-name>LoginServlet</servlet-name>
    <servlet-class>com.xinlee.demo.servlet.LoginServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>KickServlet</servlet-name>
    <servlet-class>com.xinlee.demo.servlet.KickServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>ShowOnlineUsersServlet</servlet-name>
    <servlet-class>com.xinlee.demo.servlet.ShowOnlineUsersServlet</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>LoginServlet</servlet-name>
    <url-pattern>/loginServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>KickServlet</servlet-name>
    <url-pattern>/kickServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>ShowOnlineUsersServlet</servlet-name>
    <url-pattern>/showOnlineUsersServlet</url-pattern>
  </servlet-mapping>

  <welcome-file-list>
    <welcome-file>index.jsp</welcome-file>
  </welcome-file-list>
</web-app>
